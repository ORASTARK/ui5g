{
  "name": "<%= namespace %>",
  "displayName": "<%= name %>",
  "productName": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:debug": "node_modules/.bin/gulp dev",
    "start:silent": "node_modules/.bin/gulp dev:silent",
    "start": "node_modules/.bin/gulp dev:preload",
    "build": "node_modules/.bin/gulp build:prod",
    "build:sourcemap": "node_modules/.bin/gulp build:preload"
  },
  "ui5": {
    "namespace": "<%= namespace %>",
    "build": {
      "resource": "https://<%= ui5Domain %>/1.68.1/resources/",
      "src": "./src",
      "dist": "./dist",
      "offline": false
    }
  },
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@types/openui5": "^1.40.2",
    "@ui5-next/types": "latest",
    "babel-preset-ui5-next": "^7.2.2",
    "browser-sync": "2.26.7",
    "del": "^5",
    "gulp": "^4",
    "gulp-babel": "^8",
    "gulp-filter": "^5.0.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-ui5-eager-preload": "^1.16.0",
    "http-proxy-middleware": "^0.19.1",
    "merge-stream": "^1"
  }
}
